#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/bin/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules/detect-libc/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/1.0.3/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/detect-libc/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../detect-libc/1.0.3/node_modules/detect-libc/bin/detect-libc.js" "$@"
  ret=$?
fi
exit $ret
