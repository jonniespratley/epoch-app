#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../needle/2.4.0/node_modules/needle/bin/needle" "$@"
  ret=$?
else 
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/2.4.0/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/needle/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../needle/2.4.0/node_modules/needle/bin/needle" "$@"
  ret=$?
fi
exit $ret
