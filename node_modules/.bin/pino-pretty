#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules/pino-pretty/bin.js/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules/pino-pretty/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules:/Users/jps/Github/epoch-app/node_modules/pino-pretty/bin.js/node_modules:/Users/jps/Github/epoch-app/node_modules/pino-pretty/node_modules" "$basedir/node"  "$basedir/../pino-pretty/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules/pino-pretty/bin.js/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules/pino-pretty/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/3.4.0/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/pino-pretty/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/jps/Github/epoch-app/node_modules/.pnpm/node_modules:/Users/jps/Github/epoch-app/node_modules:/Users/jps/Github/node_modules:/Users/jps/node_modules:/Users/node_modules:/node_modules:/Users/jps/Github/epoch-app/node_modules/pino-pretty/bin.js/node_modules:/Users/jps/Github/epoch-app/node_modules/pino-pretty/node_modules" node  "$basedir/../pino-pretty/bin.js" "$@"
  ret=$?
fi
exit $ret
